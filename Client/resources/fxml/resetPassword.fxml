<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="wazxse5.client.controller.ResetPasswordController">
    <children>
        <Button fx:id="resetPasswordButton" defaultButton="true" layoutX="30.0" layoutY="327.0" mnemonicParsing="false"
                onAction="#resetPassword" prefHeight="30.0" prefWidth="110.0" text="Resetuj hasło"
                AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="30.0"/>
        <GridPane hgap="5.0" layoutX="34.0" vgap="5.0" AnchorPane.bottomAnchor="120.0" AnchorPane.leftAnchor="30.0"
                  AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="80.0">
            <children>
                <Label text="Login"/>
                <TextField fx:id="loginTF" GridPane.columnIndex="1"/>
                <Label text="Mail" GridPane.rowIndex="1"/>
                <PasswordField fx:id="mailTF" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="35.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Label alignment="CENTER" layoutX="111.0" layoutY="32.0" text="Resetowanie hasła" AnchorPane.leftAnchor="30.0"
               AnchorPane.topAnchor="20.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="infoL" layoutX="54.0" layoutY="369.0" prefHeight="20.0" AnchorPane.bottomAnchor="20.0"
               AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"/>
    </children>
</AnchorPane>
