<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="wazxse5.client.controller.RegisterController">
    <children>
        <Label alignment="CENTER" layoutX="111.0" layoutY="32.0" text="Rejestracja" AnchorPane.leftAnchor="30.0"
               AnchorPane.topAnchor="20.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <GridPane hgap="3.0" vgap="3.0" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="30.0"
                  AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="60.0">
            <children>
                <Label text="Imię"/>
                <TextField fx:id="nameTF" GridPane.columnIndex="1"/>
                <Label text="Nazwisko" GridPane.rowIndex="1"/>
                <TextField fx:id="surnameTF" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Mail" GridPane.rowIndex="2"/>
                <TextField fx:id="mailTF" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Login" GridPane.rowIndex="3"/>
                <TextField fx:id="loginTF" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label text="Hasło" GridPane.rowIndex="4"/>
                <PasswordField fx:id="passwordPF" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <Label text="Powtórz hasło" GridPane.rowIndex="5"/>
                <PasswordField fx:id="passwordPF1" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="35.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Button fx:id="registerButton" defaultButton="true" layoutX="30.0" layoutY="327.0" mnemonicParsing="false"
                onAction="#register" prefHeight="30.0" prefWidth="110.0" text="Zarejestruj"
                AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="30.0"/>
        <Label fx:id="infoL" layoutX="54.0" layoutY="369.0" prefHeight="20.0" AnchorPane.bottomAnchor="20.0"
               AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"/>
    </children>
</AnchorPane>
