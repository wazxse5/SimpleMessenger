<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="wazxse5.client.controller.LoginController">
    <children>
        <Button layoutX="30.0" layoutY="327.0" mnemonicParsing="false" onAction="#loginAsUser" prefHeight="30.0"
                prefWidth="110.0" text="Zaloguj" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="30.0"/>
        <GridPane layoutX="34.0" AnchorPane.bottomAnchor="120.0" AnchorPane.leftAnchor="30.0"
                  AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="80.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="35.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Login"/>
                <TextField fx:id="loginTF" text="wazxse5" GridPane.columnIndex="1"/>
                <Label text="Hasło" GridPane.rowIndex="1"/>
                <PasswordField fx:id="passwordTF" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Adres serwera" GridPane.rowIndex="2"/>
                <TextField fx:id="serverAddressTF" text="localhost" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Port" GridPane.rowIndex="3"/>
                <TextField fx:id="serverPortTF" text="8989" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
        <Button layoutX="171.0" layoutY="327.0" mnemonicParsing="false" onAction="#loginAsGuest" prefHeight="30.0"
                prefWidth="110.0" text="Zaloguj jako gość" AnchorPane.bottomAnchor="70.0"
                AnchorPane.rightAnchor="30.0"/>
        <Label alignment="CENTER" layoutX="111.0" layoutY="32.0" prefWidth="100.0" text="Logowanie"
               AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="30.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Label fx:id="infoLabel" layoutX="54.0" layoutY="369.0" AnchorPane.bottomAnchor="30.0"
               AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"/>
   </children>
</AnchorPane>
